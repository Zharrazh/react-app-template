{
    "extends": [
      "eslint:recommended", 
      "plugin:react/recommended",
      "plugin:jsx-a11y/recommended",
      "plugin:@typescript-eslint/recommended",
      "plugin:prettier/recommended"
    ], 
    "rules": {
      "no-console": 1,
      "@typescript-eslint/no-non-null-assertion": "off"
    },
    "plugins": [
      "import", 
      "jsx-a11y"
    ],
    "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "module",
      "ecmaFeatures": {
          "jsx": true
      }
    },
    "env": {
      "es6": true,
      "browser": true,
      "node": true,
      "commonjs": true
    },
    "settings": {
      "react": {
          "version": "detect"
      }
    }
  }
  